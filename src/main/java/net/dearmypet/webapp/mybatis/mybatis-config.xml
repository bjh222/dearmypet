	</typeAliases>

	<!-- mapper 설정 -->
	<mappers>
		<mapper resource="classpath:net/dearmypet/mybatis/MemberMapper.xml" />
		<!-- <mapper resource="net/dearmypet/mybatis/MemberMapper.xml" /> -->
	</mappers>

</configuration>


